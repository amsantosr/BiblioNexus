<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="administrator">
  <inherits name='com.google.gwt.user.User' />
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/>   -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
  <!-- <inherits name='com.google.gwt.http.HTTP'/> -->
  
  <inherits name='com.googlecode.objectify.Objectify' />
  <source path="client/administrator" />
  <source path="client/services"/>
  <source path="client/entities"/>
  <source path="client/common"/>
  <source path="shared" />
  <entry-point class='com.tais.biblionexus.client.administrator.AdministratorModule' />
  <inherits name='com.tais.biblionexus.Administrator'/>
</module>
